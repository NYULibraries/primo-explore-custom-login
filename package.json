    "@babel/core": "7.19.0",
    "@babel/preset-env": "7.19.0",
    "angular": "1.8.3",
    "angular-mocks": "1.8.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-istanbul": "^6.1.1",
    "jasmine-core": "^4.4.0",
